	.slick-prev,
	.slick-next
		width 60px
		height 60px
		&:before
			content ''
			position absolute
			top 0
			bottom 0
			margin auto
			width: 20px;
			height: 32px;
			background-repeat: no-repeat;
			background-image: url(../img/sprite.png);
		span
			position absolute
			color #fff
			width 200px 
			height 20px
			display inline-block
			font-size 16px
			line-height 1
			font-family: 'AvenirNextCyrLight';
			top 0
			bottom 0
			margin auto
			transition: top 0.3s ease-out 0.3s;
		&:hover
			opacity 1
	.slick-next:before
		right 0
		background-position: right -477px;

	.slick-prev:before
		left 0
		background-position: -105px -477px;

	.slick-next:hover
		&:before
			animation: leftAnim 1s infinite;
		span
			font-size 18px
	.slick-prev:hover
		&:before
			animation: rightAnim 1s infinite;
		span
			font-size 18px
		
	.slick-prev span
		left -45px
		transform:rotate(270deg);
	.slick-next span
		right -45px
		transform:rotate(90deg);


@keyframes leftAnim {
    from {right: 0px;}
    to {right: -10px;}
}
@keyframes rightAnim {
    from {left: 0px;}
    to {left: -10px;}
}

