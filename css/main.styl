	.tit_link
		width: 100%
		li
			width 100%
			margin-bottom 2px
			a
				font-size txt15
				font-family: 'AvenirNextCyrBold';
				padding-bottom 1px
				border-bottom 1px solid #fff
				&:hover 
					border-bottom 1px solid #fff
	.s_left
		float left
		padding-left 15%
		// margin-left 110px
	.s_right
		float right
		padding-left 6%
	
	.l_h1
		color #fff
		display block
		margin auto
		text-align center
		margin-top 16%
		margin-bottom 7%
		position relative
		&:before
			content ""
			position absolute
			width 100px
			height 120px
			top -111px
			left 0
			right 0
			margin auto
			background url(../img/sprite.png) 0 -440px no-repeat
	.title
		font-size txt26
		line-height 35px 
		color #fff
		font-family: 'BanderaPro';
		text-transform uppercase
		padding 0 0 5% 20px
		margin-bottom 25px
		height 200%
		opacity 0.8
		border-left 2px solid #647585
	.slick-dots
		li
			width auto
			padding 0 25px
		button
			font-size 12px
			font-family: 'AvenirNextCyrRegular';
			width auto
			height 37px
			color #737c99
			border-top 3px solid transparent
			text-transform uppercase
			position relative
		li.slick-active button
			color #fff
			border-top 3px solid light_blue
			&:before
				content ""
				position absolute
				width 11px
				height 6px
				background url(../img/sprite.png) right -311px no-repeat
				top - 9px
				left 0
				right 0
				margin auto
	.slick-prev,
	.slick-next
		span
			position absolute
			color #fff
			width 200px 
			height 20px
			display inline-block
			font-size 16px
			line-height 1
			font-family: 'AvenirNextCyrLight';
			top 6px
			transition: top 0.3s ease-out 0.3s;
	.slick-prev span
		left -45px
		transform:rotate(270deg);
	.slick-next span
		right -45px
		transform:rotate(90deg);


@keyframes leftAnim {
    from {right: 0px;}
    to {right: -10px;}
}
@keyframes rightAnim {
    from {left: 0px;}
    to {left: -10px;}
}

.main_slider
	.slick-prev,
	.slick-next
		width 60px
		height 30px
		&:before
			content ''
			position absolute
			top 0
			width: 20px;
			height: 32px;
			background-repeat: no-repeat;
			background-image: url(../img/sprite.png);

		&:hover
			opacity 1
	.slick-next:before
		right 0
		background-position: right -477px;

	.slick-prev:before
		left 0
		background-position: -105px -477px;

	.slick-next:hover
		&:before
			animation: leftAnim 1s infinite;
		span
			font-size 18px
	.slick-prev:hover
		&:before
			animation: rightAnim 1s infinite;
		span
			font-size 18px





