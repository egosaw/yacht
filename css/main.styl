	.slick-dots
		li
			width auto
			padding 0 25px
		button
			font-size 12px
			font-family: 'AvenirNextCyrRegular';
			width auto
			height 37px
			color #737c99
			border-top 3px solid transparent
			text-transform uppercase
			position relative
			&:hover
				color #fff
		li.slick-active button
			color #fff
			border-top 3px solid light_blue
			&:before
				content ""
				position absolute
				width 11px
				height 6px
				background url(../img/sprite.png) right -311px no-repeat
				top - 9px
				left 0
				right 0
				margin auto
	.slick-prev,
	.slick-next
		width 60px
		height 100px
		padding 50px
		box-sizing content-box
		&:before
			content ''
			position absolute
			top 0
			bottom 0
			margin auto
			width: 20px;
			height: 32px;
			background-repeat: no-repeat;
			background-image: url(../img/sprite.png);
		span
			position absolute
			color #fff
			width 200px 
			height 20px
			display inline-block
			font-size 16px
			line-height 1
			font-family: 'AvenirNextCyrLight';
			top 0
			bottom 0
			margin auto
			transition: top 0.3s ease-out 0.3s;
		&:hover
			opacity 1
	.slick-next:before
		right 40px
		background-position: right -477px;

	.slick-prev:before
		left 40px
		background-position: -105px -477px;

	.slick-next:hover
		&:before
			animation: leftAnim 0.5s infinite;
			animation-direction: alternate
			animation-timing-function: ease-in
		span
			font-size 18px
	.slick-prev:hover
		&:before
			animation: rightAnim 0.5s infinite;
			animation-direction: alternate
			animation-timing-function: ease-in
		span
			font-size 18px
		
	.slick-prev span
		left 5px
		transform:rotate(270deg);
	.slick-next span
		right 5px
		transform:rotate(90deg);


@keyframes leftAnim {
    from {right: 40px;}
    to {right: 30px;}
}
@keyframes rightAnim {
    from {left: 40px;}
    to {left: 30px;}
}

