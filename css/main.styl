.cs-select 
	display: inline-block;
	vertical-align: middle;
	position: relative;
	text-align: left;
	background: #fff;
	z-index: 5;
	width: 100%;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	span
		display: block;
		position: relative;
		cursor: pointer;
		padding: 1em;
		white-space: nowrap;
		overflow: hidden;
		text-overflow: ellipsis;
		box-sizing border-box
	.cs-placeholder
		width 100%
		&:after
			right: 1em;
			width 16px
			height 13px
			background url(../img/icon/arrow_form_white.png) 0 0 no-repeat
		&:hover:after
			background url(../img/icon/arrow_form_blue.png) 0 0 no-repeat
	.cs-options
		max-height 324px
		position: absolute;
		top: -2px;
		left: -2px;
		border 2px solid transparent
		width: 100%;
		display none
		opacity: 1;
		overflow-y: scroll;
		overflow-x hidden
		background: transparent;
		visibility: visible;
		// padding-bottom: 1.25em;
		pointer-events: none;
		ul
			width 100%
			span
				padding 1em
				width 100%
			li
				width 100%
				border-bottom 2px solid #fff
				background: #020622
				&:last-of-type
					border-bottom 0
				&:hover 
					background #fff
	&.cs-active
		.cs-options
			border 2px solid #fff
.cs-select.cs-active .cs-options {
	visibility: visible;
	display block
}

.cs-select > .cs-placeholder:after,
.cs-select .cs-selected span:after {
	content ""
	speak: none;
	position: absolute;
	top: 50%;
	-webkit-transform: translateY(-50%);
	transform: translateY(-50%);
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.cs-active
	z-index 10



.cs-select ul li.cs-focus span {
	background-color: #ddd;
}

/* Optgroup and optgroup label */
.cs-select li.cs-optgroup ul {
	padding-left: 1em;
}

.cs-select li.cs-optgroup > span {
	cursor: default;
}

.cs-placeholder:hover{
	background: #fff;
	color: #000;
}





































