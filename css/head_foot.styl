// ============================ MAIN PAGE =============================
header
	position relative
	height 150px

.scroll
	position absolute
	width 100%
	top 0
	left 0
	display inline-block
	z-index 2
	transition: top 0.3s ease-out 0.3s;
.main_wrap
	max-width 1600px
	min-width 1280px
	display block
	margin auto


.header,
.content{
	box-sizing border-box
	padding 0 3.125%
}


.header
	width 100%
	background-color dark_blue
	position relative
	background-repeat no-repeat
	height 100px
	margin-bottom 50px
	


.header,
.main_slider
	width 100%
	display block



















