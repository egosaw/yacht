body
	font-size inner_text
	color txt_main
	background #ccc
	font-family 'AvenirNextCyrRegular', Arial, Helvetica, sans-serif

.main_wrap
	max-width 1600px
	min-width 1280px
	display block
	margin auto
.header,
.content{
	box-sizing border-box
	padding 0 3.125%
}
.header
	width 100%
	background-color dark_blue
	position relative
	background-repeat no-repeat
	height 100px
	
.header,
.main_slider
	width 100%
	display block



















// font style

.fs1
	font-size txt14
	color #fff

.fs2
	font-size txt14
	text-transform uppercase
	color #000
	font-weight 900
	font-family 'AvenirNextCyrBold' Arial, Helvetica, sans-serif

.fs3
	font-size 77px
	font-family 'AvenirNextCyrBold' Arial, Helvetica, sans-serif
	font-weight 900
.fs4
	text-transform uppercase
	color title_col
	font-size txt14
.fs5
	color #020722
	font-size 63px
	font-weight 900
	font-family 'AvenirNextCyrBold' Arial, Helvetica, sans-serif
	
.fs6
	font-size txt29
	color #020722
	font-weight 700

.fs7
	font-size txt14
	letter-spacing 0.2px
	text-transform uppercase
	font-family: 'BanderaPro';
	color #020722


.tac
	text-align center
.tar
	text-align right
.tal
	text-align left













.content
	max-width 1600px
	display block
	margin auto
	background-color #fff




.b_line:hover
	border-bottom 2px solid light_blue
.login_in
	display inline-block
	padding-left 49px
	margin-top 39px
	color dis_txt
	font-size txt15
	position relative
	&:before
		content ""
		position absolute
		width 28px
		height 33px
		left 0px
		top -8px
		background url(../img/sprite.png) right -339px no-repeat

.sign_up,
.sign_in
	color dis_txt
	&:hover
		color #fff
.line_dis
	content '/'
	margin 0 5px 0 8px
	color dis_txt
	font-weight 700

.social
	margin 40px 0 0 10.3%
	display inline-block
	li
		display inline-block
		margin 0 31px 0 0
		a
			display inline-block
			width 16px
			height 14px
			padding 0 0 3px 0
			background-image url(../img/sprite.png)
			&.fb
				background-position right -387px
			&.ig
				background-position right -404px
			&.yt
				background-position right -421px

.logo
	position absolute
	top 7px
	left 0
	right 0
	bottom 0
	margin auto
	width 145px
	height 70px
	background url(../img/sprite.png) center 0 no-repeat

.mini_menu
	display inline-block
	margin 40px 0 0 25.7%
	width 41%
	li
		margin 0 16% 0 0
		&:last-of-type
			margin 0
		a
			font-size txt14

.phone
	display inline-block
	font-size txt14
	color #fff
	margin 32px 0 0 0
	padding 9px 28px
	letter-spacing 0.65px
	background-color light_blue
	border-radius 3px
	float right
	cursor pointer
.head_left
	width 50%
	display inline-block
	float right
	
	
.main_menu
	width 100%
	display block
	background-color #fff
	text-align center
	height 50px
	position relative
	li 
		display inline-block
		margin 17px 33px 0 33px
		a
			font-size inherit
			color inherit
	.search
		position absolute
		width 16px
		height 16px
		background url(../img/sprite.png) right -437px no-repeat
		right 3.1%
		top 18px
		input
			display none

.img,
.box
	background-size cover
	background-repeat no-repeat
	background-position center center
.main_slider
	height 811px
	width 100%
	display block
	margin 0 0 40px 0
	// overflow hidden
	.slick-slide
		position relative
		height 811px
	.img
		width 100%
		height 100%
		top 0
		left 0
		display block

		position absolute
		z-index -1
	.slick-dots
		height 40px
		bottom -40px
		background-color #0B0B0B
	.slide_cont
		width 1000px
		margin auto
		a
			display block
	.s_left,
	.s_right
		// width 49.5%
		display inline-block
	.s_left
		float left
		margin-left 110px
	.s_right
		float right
	
	.l_h1
		color #fff
		display block
		margin auto
		text-align center
		margin-top 170px
		margin-bottom 65px
		position relative
		&:before
			content ""
			position absolute
			width 100px
			height 120px
			top -111px
			left 0
			right 0
			margin auto
			background url(../img/sprite.png) 0 -440px no-repeat
	.title
		font-size txt26
		line-height 35px 
		color #fff
		font-family: 'BanderaPro';
		text-transform uppercase
		padding 0 0 5% 20px
		height 200%
		border-left 2px solid #647585
.slick-prev,
.slick-next
	&:hover
		opacity 1


.slick-slider.main_slider
	margin-bottom 40px
	
.big_title
	padding-top 59px
	margin auto
	&.tac
		width 520px
		text-align left
	&.tal
		padding-left 7%
	.fs4
		text-align left
		width 100%
		display inline-block
		padding 0 0 25px 10px
		border-left 2px solid tit_bor

.row, .row1
	display block
	width 100%
	overflow hidden
.row 
	.box, .yacht_img
		height 420px
.row1 
	.box, .yacht_img
		height 380px
.cell50
	width 50%
	// margin 0 -0.2% 0 0
	display inline-block
	margin 0
	float left

.banner
	width 100%
	max-height 71px
	padding 0 0 15px 22%
	margin-top 50px
	margin-bottom 20px
	position relative
	display inline-block
	box-sizing border-box
	&:before
		content ""
		position absolute
		left 0
		top 0
		bottom 0
		margin auto
		display inline-block
		width 20%
		height 1px
		background-color tit_bor
	.fs7
		display inline-block
		margin 4px 0 0 2px
.box
	width 100%
	height 420px


.yacht_img
	width 56%
	float left
.yacht_param
	width 44%
	float left
	