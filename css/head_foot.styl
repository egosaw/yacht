// ============================ PLACEHOLDER CUSOTOM=============================
input::-webkit-input-placeholder,
input:-moz-placeholder,
input::-moz-placeholder,
input:-ms-input-placeholder,
input.placeholder{
   
}

ul,ol
	margin 0
	padding 0
	// list-style-type none

li
	display inline-block

p
	margin 0
input,textarea,select,button
	border 0

*:focus
	outline none

*
	vertical-align top

a
	text-decoration none
	color #fff

input
	width 100%
	height 100%
	display block
	background-color transparent
	margin 0
	padding-right 25px
	padding-left 8px
	box-sizing border-box
	border 1px solid col_grey


.none
	display none

.fleft
	float left
.fright
	float right

// ico classes


.btn
	display inline-block
	padding 9px
	border 1px solid col_grey
	color #595A5C
	box-shadow 0 3px 2px 0px #D6D7D8
	background-color #fff
	font-weight 700
	margin-left 10px
	&.apply,
	&.private
		padding 9px 9px 8px 30px
	&.private
		background col_red
		border-color col_red
		color #fff
		&:before
			width 11px
			height 13px
			top 9px
			left 8px
			background-position 0 -39px
	&.apply:before
		width 10px
		height 13px
		top 9px
		left 9px
		background-position 0 -23px

@font-face {
    font-family: 'AvenirNextCyrRegular';
    src: url('../fonts/AvenirNextCyrRegular.eot');
    src: url('../fonts/AvenirNextCyrRegular.eot') format('embedded-opentype'),
         url('../fonts/AvenirNextCyrRegular.woff2') format('woff2'),
         url('../fonts/AvenirNextCyrRegular.woff') format('woff'),
