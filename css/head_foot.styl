// ============================ BASE STYLE =============================
ul,ol
	margin 0
	padding 0
	// list-style-type none

li
	display inline-block

p
	margin 0

input,textarea,select,button
	border 0

*:focus
	outline none

*
	vertical-align top

a
	text-decoration none
	color #fff

