.content
	width 100%
	max-width 1600px
	display block
	margin auto
	background-color #fff




.b_line
	border-bottom 2px solid light_blue
	&:hover
		border-bottom 2px solid transparent
.login_in
	display inline-block
	padding-left 49px
	margin-top 39px
	color dis_txt
	font-size txt15
	position relative
	&:before
		content ""
		position absolute
		width 28px
		height 33px
		left 0px
		top -8px
		background url(../img/sprite.png) right -154px no-repeat

.sign_up,
.sign_in
	color dis_txt
	&:hover
		color #fff
.line_dis
	content '/'
	margin 0 5px 0 8px
	color dis_txt
	font-weight 700

.social
	margin 40px 0 0 10.3%
	display inline-block
	li
		display inline-block
		margin 0 31px 0 0
		a
			display inline-block
			width 16px
			height 14px
			padding 0 0 3px 0
			background-image url(../img/sprite.png)
			&.fb
				background-position 104px 0
			&.ig
				background-position 1px 0
			&.yt
				background-position 122px 0
				
.logo
	position absolute
	top 7px
	left 0
	right 0
	bottom 0
	margin auto
	width 145px
	height 70px
	z-index 2
	background url(../img/sprite.png) center -484px no-repeat

@keyframes phone {
    from {padding: 9px 28px}
    to {padding: 9px 46px 9px 10px}
}
.mini_menu
	display inline-block
	margin 40px 0 0 25.7%
	width 41%
	li
		margin 0 16% 0 0
		&:last-of-type
			margin 0
		a
			font-size txt14

.phone
	display inline-block
	font-size txt14
	color #fff
	margin 32px 8% 0 0
	padding 9px 28px
	letter-spacing 0.65px
	background-color light_blue
	border-radius 3px
	float right
	cursor pointer
	transition 0.7s
	position relative
	// transition: top 1s ease-out 1s;
	&:before
		content ""
		width 16px
		height 13px
		background url(../img/sprite.png) left -89px no-repeat
		opacity 0
		position absolute
		top 0
		bottom 0
		margin auto
		right 28px
		transition 0.5s
	&:hover
		padding: 9px 46px 9px 10px
		&:before
			right 15px
			opacity 1

.head_left
	width 50%
	display inline-block
	position absolute
	left 50%
	top 0
	&:after
		clear()
		
	
.main_menu
	width 100%
	display block
	// background-color #f2f2f2
	background-color #fff
	text-align center
	height 50px
	position absolute
	top 100px
	left 0
	z-index 2
	li 
		display inline-block
		margin 17px 33px 0 33px
		a
			font-size inherit
			color inherit
			font-family: 'AvenirNextCyrBold';
			&:hover
				color light_blue
	.search
		position absolute
		width 16px
		height 16px
		background url(../img/sprite.png) right -105px no-repeat
		right 3.1%
		top 18px
		&:hover
			background url(../img/sprite.png) right -78px no-repeat
		input
			display none

.img,
.box
	background-size cover
	background-repeat no-repeat
	background-position center center






















footer
	width 100%
	margin-top 55px
	padding 106px 0 100px 0
	background-color foot_col
	overflow hidden
	position relative

.flogo
	width 23%
	float left
	display inline-block
	// height 100%
	height 200px
	background url(../img/sprite.png) center -581px no-repeat


.info_block
	display inline-block
	width 77%
	float left
	margin-top 63px

.contact
	width 150px
	display inline-block
	font-size txt14
.f_menu
	display inline-block
	padding 0 0 0 8%
	// width 155px
	p
		font-family: 'AvenirNextCyrBold';
		font-size txt16
		margin 0 0 10px 0
	li
		display block
		margin 0 0 10px 0
		a
			color #000
			&:hover
				text-decoration underline
.fsocial
	position absolute
	left 22%
	top 80px
	li
		padding 0 10px
	a
		width 20px
		height 20px
		padding-bottom 3px
		display inline-block
		border-bottom 2px solid transparent
		&:hover
			border-bottom 2px solid light_blue
		&.fb
			background url(../img/sprite.png) 0 -31px no-repeat
		&.ig
			background url(../img/sprite.png) -30px -31px no-repeat
		&.sk
			background url(../img/sprite.png) -60px -31px no-repeat
		&.yt
			background url(../img/sprite.png) -90px -31px no-repeat
		&.gp
			background url(../img/sprite.png) -125px -31px no-repeat








