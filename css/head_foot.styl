.main_slider
	height 811px
	width 100%
	display block
	margin 0 0 40px 0
	overflow hidden
	.slick-slide
		position relative
		height 811px
		margin-bottom 40px
	.img
		width 100%
		height 100%
		top 0
		left 0
		display block

		position absolute
		z-index -1
	.slick-dots
		height 40px
		bottom 0
		background-color #0B0B0B
		
	.slide_cont
		width 1000px
		margin auto
		a
			display block
	.s_left,
	.s_right
		// width 49.5%
		display inline-block
	.s_left
		float left
		margin-left 110px
	.s_right
		float right
	
	.l_h1
		color #fff
		display block
		margin auto
		text-align center
		margin-top 170px
		margin-bottom 65px
		position relative
		&:before
			content ""
			position absolute
			width 100px
			height 120px
			top -111px
			left 0
			right 0
			margin auto
			background url(../img/sprite.png) 0 -440px no-repeat
	.title
		font-size txt26
		line-height 35px 
		color #fff
		font-family: 'BanderaPro';
		text-transform uppercase
		padding 0 0 5% 20px
		height 200%
		border-left 2px solid #647585
	.slick-dots
		li
			width auto
			padding 0 25px
		button
			font-size 12px
			font-family: 'AvenirNextCyrRegular';
			width auto
			height 37px
			color #737c99
			border-top 3px solid transparent
			text-transform uppercase
			position relative
		li.slick-active button
			color #fff
			border-top 3px solid light_blue
			&:before
				content ""
				position absolute
				width 11px
				height 6px
				background url(../img/sprite.png) right -311px no-repeat
				top - 9px
				left 0
				right 0
				margin auto
	.slick-prev,
	.slick-next
		span
			position absolute
			color #fff
			width 150px 
			height 20px
			display inline-block
			font-size 16px
			line-height 1
			font-family: 'AvenirNextCyrLight';
			top 6px
	.slick-prev span
		left -22px
		transform:rotate(270deg);
	.slick-next span
		right -22px
		transform:rotate(90deg);
			
.slick-prev,
.slick-next
	&:hover
		opacity 1



