// ============================ MAIN PAGE =============================

header,
.header,
.main_slider
	width 100%
	display block


header
	position relative
	height 150px
	min-width 1280px
	vertical-align top
.scroll
	position absolute
	width 100%
	min-width 1280px
	top 0
	left 0
	display inline-block
	z-index 20
	// transition: top 0.1s ease-out 0.1s;

.main_wrap
	max-width 100%
	// max-width 1600px
	min-width 1280px
	display block
	margin auto


.header,
.content{
	box-sizing border-box
	padding 0 3.125%
	min-width 1280px
}


.narrow_block
	width 75%
	display block
	margin auto


.header
	background-color dark_blue
	position relative
	background-repeat no-repeat
	height 100px
	margin-bottom 50px



















