.none
	display none

.fleft
	float left

.fright
	float right


.btn
	display inline-block
	padding 9px
	border 1px solid col_grey
	color #595A5C
	box-shadow 0 3px 2px 0px #D6D7D8
	background-color #fff
	font-weight 700
	margin-left 10px

