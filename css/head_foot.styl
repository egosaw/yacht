// ============================ BASE STYLE =============================
ul,ol
	margin 0
	padding 0
	// list-style-type none

li
	display inline-block

p
	margin 0

input,textarea,select,button
	border 0

*:focus
	outline none

*
	vertical-align top

a
	text-decoration none
	color #fff

input
	width 100%
	height 100%
	display block
	background-color transparent
	margin 0
	padding-right 25px
	padding-left 8px
	box-sizing border-box
	border 1px solid col_grey


.none
	display none

.fleft
	float left

.fright
	float right


.btn
	display inline-block
	padding 9px
	border 1px solid col_grey
	color #595A5C
	box-shadow 0 3px 2px 0px #D6D7D8
	background-color #fff
	font-weight 700
	margin-left 10px


